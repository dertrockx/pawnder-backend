	"description": "Awesome project developed with TypeORM.",
	"devDependencies": {
		"@types/faker": "^5.5.8",
		"@types/node": "^16.10.2",
		"faker": "^5.5.3",
		"ts-node": "^10.9.2",
		"typescript": "^4.9.5"
	},
	"dependencies": {
		"ts-node": "^10.9.2",
		"typescript": "^4.9.5",
		"@types/bcrypt": "^5.0.2",
		"@types/cookie-parser": "^1.4.7",
		"@types/express": "^4.17.21",
		"@types/jest": "^27.5.2",
		"@types/multer": "^1.4.11",
		"@types/npm": "^7.19.3",
		"bcrypt": "^5.1.1",
		"body-parser": "^1.20.2",
		"cloudinary": "^1.41.3",
		"cookie-parser": "^1.4.6",
		"dotenv": "^10.0.0",
		"express": "^4.19.2",
		"jest": "^27.5.1",
		"jsonwebtoken": "^8.5.1",
		"morgan": "^1.10.0",
		"multer": "^1.4.4",
		"mysql": "^2.14.1",
		"rand-token": "^1.0.1",
		"reflect-metadata": "^0.2.2",
		"ts-jest": "^27.1.5",
		"tsconfig-paths": "^3.15.0",
		"typeorm": "0.3.20"
	},
	"scripts": {
		"start": "ts-node --transpile-only -r tsconfig-paths/register src/index.ts",
		"seed": "ts-node -r tsconfig-paths/register src/seed.ts",
		"dev:server": "nodemon --watch './**/*.ts' --exec 'ts-node -r tsconfig-paths/register' src/index.ts",
		"typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm",
		"schema:sync": "yarn typeorm schema:sync",
		"docker": "docker exec -it pawnder-db bash",
		"test": "jest --runInBand --verbose"
	}
}
